<header class="page-header">
<h1>Admin &raquo; Dashboard</h1>
</header>

<div class="panel panel-default products">
  <header class="page-header">
    <h1>
    Product Quantities
    </h1>
  </header>

  <div class="products">
  <table class="table table-bordered">
      <thead>
        <tr>
          <th>Category</th>
          <th>Quantity</th>
        </tr>
      </thead>
      <tbody>
      <% @categories.each do |item| %>
        <%= render 'category', category: item[0], quantity: item[1]  %>        
      <% end %>
      </tbody>
    </table>
  </div>
</div>